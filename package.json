{
  "name": "soundbread-nodejs",
  "version": "0.0.30",
  "description": "Soundbread NodeJS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "jshint": "jshint -c .jshintrc --exclude-path .jshintignore .",
	"imgsize": "FAIL=0; for FILE in webroot/img/*; do identify -format '%i is %[fx:w]x%[fx:h] %C' \"$FILE\" | grep -v '190x190 JPEG' && FAIL=1; done; exit $FAIL",
    "test": "npm run jshint && npm run imgsize"
  },
  "pre-commit": [
    "jshint",
    "imgsize"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SoundBread/soundbread-nodejs"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SoundBread/soundbread-nodejs/issues"
  },
  "dependencies": {
    "console-stamp": "^0.2.4",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "server-store": "^1.0.2",
    "socket.io": "^1.4.6"
  },
  "devDependencies": {
    "jshint": "~2.9.2",
    "pre-commit": "~1.1.3"
  }
}
